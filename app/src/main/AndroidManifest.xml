<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kc.kproejct">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name="com.kc.a50_android_hacks.BitmapSizeTestActivity" android:label="000图片大小压缩测试">
            <intent-filter>
                <action android:name="com.kc.life.rise" />
            </intent-filter>
        </activity>


        <activity android:name=".KImageLoaderActivity" android:label="000imageloader">
            <intent-filter>
                <action android:name="com.kc.life.rise" />
            </intent-filter>
        </activity>
    </application>

</manifest>